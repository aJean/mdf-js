    "format": "prettier --config ./.prettierrc.js --write ./packages/**/*.{ts,tsx}",
    "pub": "lerna publish"
  },
  "devDependencies": {
    "@types/node": "14.14.7",
    "father-build": "1.19.1",
    "ts-node": "9.0.0",
    "typescript": "4.0.5"
  },
  "repository": "https://github.com/aJean/mdf-js.git",
  "version": "0.0.0",
  "description": "mdf js framework",
